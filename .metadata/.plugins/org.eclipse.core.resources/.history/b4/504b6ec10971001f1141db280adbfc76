package numbers;

public class CalculateProfit 
{
	public static int fee(int hours,int minutes)
	{
		if(hours<0)
		{
			return -1;
		}
		else if (minutes<0) {
			return -2;
		}
		else if (minutes<=15) {
			return 0;
		}
		else if (minutes>15&&hours<=2&&minutes==0) {
			return 5;
		}
		else if (hours>=2&&hours<=5) {
			return 10;
		}
		else {
			return 10+1*hours;
		}
	}
public static void main(String[] args) {
	System.out.println(fee(4,50));
}
}
